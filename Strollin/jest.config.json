
{
  "preset": "react-native",
  "setupFiles": [
    "./node_modules/react-native-gesture-handler/jestSetup.js",
    "<rootDir>/setupTests.js"
  ],
  "globals": {
    "__DEV__": true
  },
  "transform": {
    "^.+\\.js$": "babel-jest"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!react-native|react-navigation)/"
  ],
  "coveragePathIgnorePatterns": [
		"/node_modules/",
    "/jest",
    "/Store/",
    "/apiServer",
    "/Translation",
    "/Components/map.js",
    "/Components/Socket.js"
	]
}